<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbmsproject.ignite.tutorials.model.StudentAttendance" table="studentAttendance">
        <composite-id>
		    <key-property name="RollNo">
		    	<column name="RollNo" />
		    </key-property>
		    <key-property name="Date">
		    	<column name="Date" />
	    	</key-property>
		</composite-id>
        <property name="isPresent">
            <column name="isPresent" />
        </property>
        <property name="CourseId">
            <column name="CourseId" />
        </property>
        
        <property name="BatchId">
            <column name="BatchId" />
        </property>
    </class>
</hibernate-mapping>