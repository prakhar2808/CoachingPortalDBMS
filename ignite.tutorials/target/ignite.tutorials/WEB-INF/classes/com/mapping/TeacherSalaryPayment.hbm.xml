<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dbmsproject.ignite.tutorials.model.TeacherSalaryPayment" table="teacherSalaryPayment">
        <composite-id>
		    <key-property name="TransactionNo">
		    	<column name="TransactionNo" />
		    </key-property>
		</composite-id>
        <property name="Amount">
            <column name="Amount" />
        </property>
        <property name="Date">
            <column name="Date" />
        </property>
        <property name="TeacherId">
            <column name="TeacherId" />
        </property>
    </class>
</hibernate-mapping>